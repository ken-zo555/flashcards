<% if user_signed_in? %>
  <div class="center jumbotron", style="background-color: rgba(60, 60, 60, .5); margin-top: 0.5rem" >
    <% breadcrumb :check %>
    <h3>FLASH CARDS</h3>
    <p>Select Ring for Check!<p>
    <%= render 'rings/rings_select_for_check', rings: @rings %>
  </div>
<% else %>
  <div class="container">
    <div class="content-center brand">
      <h3>WELLCOME TO</h3>
      <h1>FLASH CARDS</h1>
    </div>
  </div>
<% end %>